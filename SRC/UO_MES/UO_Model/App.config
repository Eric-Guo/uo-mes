<?xml version="1.0"?>
<!--  Please note this file is spaced by 4 space, not 2 space
      If your vs.net environment is configured in 2 space format,
      You can configure in Tools->Options->Text Editor->XML Format->Tab.
 -->
<configuration>
    <configSections>
        <section name="openaccess" type="Telerik.OpenAccess.Config.ConfigSectionHandler, Telerik.OpenAccess.Config, Version=1.0.0.0, Culture=neutral, PublicKeyToken=7ce17eeaf1d59342" />
    </configSections>
    <openaccess xmlns="http://www.telerik.com/OpenAccess">
        <references />
        <connections>
            <!-- Default DB connection: MESDatabaseConnection Should Always Exist
                 Copy following existing db configuration to MESDatabaseConnection, 
                 MES will use this connection as primary DB connection. -->
            <connection id="MESDatabaseConnection">
                <databasename>UO_MES</databasename>
                <servername>(LOCAL)\SQLEXPRESS</servername>
                <integratedSecurity>True</integratedSecurity>
                <backendconfigurationname>mssqlConfiguration</backendconfigurationname>
            </connection>
            <connection id="MES_SQLExpress_DBConnection">
                <databasename>UO_MES</databasename>
                <servername>(LOCAL)\SQLEXPRESS</servername>
                <integratedSecurity>True</integratedSecurity>
                <backendconfigurationname>mssqlConfiguration</backendconfigurationname>
            </connection>
            <connection id="MES_Ora_DBConnection">
                <!-- databasename is oracle service name -->
                <databasename>OCA.ericguo.local</databasename>
                <servername>localhost</servername>
                <integratedSecurity>False</integratedSecurity>
                <backendconfigurationname>oracleConfiguration</backendconfigurationname>
                <user>orm_user</user>
                <password>orm_user</password>
            </connection>
        </connections>
        <backendconfigurations>
            <backendconfiguration id="mssqlConfiguration" backend="mssql">
                <mappingname>commonMapping</mappingname>
            </backendconfiguration>
            <backendconfiguration id="oracleConfiguration" backend="oracle">
                <mappingname>commonMapping</mappingname>
            </backendconfiguration>
        </backendconfigurations>
